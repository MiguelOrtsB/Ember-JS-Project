<h2 class="list-title">Lista de Usuarios</h2>
<table class="users-table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Phone</th>
            <th>Country</th>
            <th>Mail</th>
        </tr>
    </thead>
    <tbody>
        {{#each @users as |user|}}
            <tr>
                <td>{{user.username}}</td>
                <td>{{user.phone}}</td>
                <td>{{user.country}}</td>
                <td>{{user.mail}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>